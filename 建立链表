#include <stdio.h>
#include <string.h>
#include <stdlib.h>

void main()
{
 struct node
{ int  date;
  struct node *next;
};
struct node *s,*head,*g;
s=(struct node *)malloc(sizeof(struct node));
  s->date=17;
head=s;

g=(struct node *)malloc(sizeof(struct node));
  g->date=20;
  g->next=NULL;
  s->next=g;

 int sum=0;
s=head;
while (s!=NULL)
{sum+=s->date;
s=s->next;}
printf("%d\n",sum);
}


